<%= form_for @personal_info, url: wizard_path , method: :put do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.object.errors.full_messages_for(:name)&.first %>

  <%= f.label :contact_no %>
  <%= f.text_field :contact_no %>
  <%= f.object.errors.full_messages_for(:contact_no)&.first %>
  
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.object.errors.full_messages_for(:address)&.first %>

  <%= f.submit 'Next' %>
<% end %>
